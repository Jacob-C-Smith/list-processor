{
    "type" : "stream processor",
    "description" : "Add A to B; Store in O",
    "nodes" :
    {
        "A" :
        {
            "out"  : [ "A" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/a.stream"
            }
        },
        "B" :
        {
            "out"  : [ "B" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/b.stream"
            }
        },
        "P" :
        {
            "in"   : [ "A", "B" ],
            "out"  : [ "O" ],
            "data" :
            {
                "type" : "program",
                "task" : "O <- A + B"
            }
        },
        "O" :
        {
            "in" : [ "O" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/o.stream"
            }
        }
    },
    "connections" : 
    [
        [ "A:A", "P:A" ],
        [ "B:B", "P:B" ],
        [ "P:O", "O:O" ]
    ]
}