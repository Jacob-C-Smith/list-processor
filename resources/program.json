{
    "type" : "stream processor",
    "description" : "Add A to B; Store in O",
    "nodes" :
    {
        "A" :
        {
            "out"  : [ "a" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/a.stream"
            }
        },
        "B" :
        {
            "out"  : [ "b" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/b.stream"
            }
        },
        "P" :
        {
            "in"   : [ "a", "b" ],
            "out"  : [ "o" ],
            "data" :
            {
                "type"    : "program",
                "program" : [ { "o" : "(+ a b)" }]
            }
        },
        "O" :
        {
            "in" : [ "o" ],
            "data" :
            {
                "type" : "file",  
                "path" : "resources/o.stream"
            }
        }
    },
    "connections" : 
    [
        [ "A:a", "P:a" ],
        [ "B:b", "P:b" ],
        [ "P:o", "O:o" ]
    ]
}